{
  "name": "myproject/telegram-bot",
  "description": "Telegram bot + backend на PHP 5.6",
  "require": {
    "php": ">=5.6,<7.0",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_sqlite": "*",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "6.*",
    "ext-redis": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Core\\": "core/",
      "Config\\": "config/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
